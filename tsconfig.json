{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es6",
        "jsx": "react",
        "experimentalDecorators": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "exclude": [
        "dist",
        "node_modules",
        "wwwroot"
    ],
    "include": [
        "src/**/*"
    ]
}