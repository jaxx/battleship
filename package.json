    "simple-git": "^1.54.0",
    "socket.io": "^1.5.0"
  },
  "devDependencies": {
    "@types/body-parser": "^0.0.33",
    "@types/bootstrap": "^3.3.32",
    "@types/express": "^4.0.33",
    "@types/jquery": "^2.0.33",
    "@types/jqueryui": "^1.11.31",
    "@types/socket.io": "^1.4.27",
    "@types/socket.io-client": "^1.4.27",
    "awesome-typescript-loader": "^2.2.4",
    "babel-core": "^6.17.0",
